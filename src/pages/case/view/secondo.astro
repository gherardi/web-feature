---
import Layout from '../../../layouts/Layout.astro';
const title = 'View Transition';
---

<Layout title={title}>
	<main>
		<div class='max-w-6xl mx-auto'>
			<div class='aspect-video overflow-hidden'>
				<img
					transition:name='terzo'
					src='https://www.pbs.org/wnet/nature/files/2014/09/ExtraordinaryCats-Main-900x506.jpg'
						alt='cat'
					class='object-cover'
				/>
			</div>
		</div>
	</main>
</Layout>
